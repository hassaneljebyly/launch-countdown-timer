    "url": "https://github.com/hassaneljebyly/launch-countdown-timer/issues"
  },
  "homepage": "https://github.com/hassaneljebyly/launch-countdown-timer#readme",
  "browserslist": [
    "last 4 version"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.28",
    "postcss-assets": "^6.0.0",
    "postcss-cli": "^10.1.0",
    "sass": "^1.66.0"

{
  "name": "launch-countdown-timer",
  "version": "1.0.0",
  "description": "This is a solution to the Launch countdown timer challenge on Frontend Mentor",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss src/styles/main.css -o public/styles/main.min.css --watch",
    "compile:scss": "sass --watch --no-source-map src/styles/sass/main.scss src/styles/main.css",
    "Dev": "npm run compile:scss && npm run build:css"
  },
